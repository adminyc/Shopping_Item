<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wykl.dao.Front1Dao">

    <select id="loginkl" resultType="Users">
		SELECT 	`userid`, `username`, `loginname`, `password`, `documenttype`, `idnumber`, `mobilephoneno`, `email`,
		`gender`, `zipcode`, `province`, `thecity`, `address`, `integeral`, `registrationtime`, `birthday`,
		`userstate`, `membershipgrade`, `money`, `ismerchants`, `isadmin`, `encrypted`, `encryptedanswer`
		FROM `course`.`users`
		WHERE `mobilephoneno` = #{mobilephoneno}  AND `password`= #{password}
	</select>

</mapper>